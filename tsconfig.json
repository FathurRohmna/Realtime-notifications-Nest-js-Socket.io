{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "importHelpers": true,
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "incremental": true,
    "outDir": "./dist",
    "baseUrl": "."
  },
  "include": ["src/**/*.ts", "test/**/*.ts", "prisma/*.ts"]
}
